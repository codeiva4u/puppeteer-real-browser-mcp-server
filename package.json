{
  "name": "puppeteer-real-browser-mcp-server",
  "version": "1.0.14",
  "description": "MCP server for puppeteer-real-browser",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "npm run test:quick",
    "test:quick": "jest --testTimeout=30000",
    "test:full": "cd tests/mcp-testing && npm test",
    "test:performance": "node tests/performance/performance-tests.js",
    "test:integration": "echo 'Use Claude Code CLI with test prompts in tests/mcp-testing/claude-prompts/'",
    "test:debug": "node debug-server.js",
    "test:all": "npm run test:quick && npm run test:full && npm run test:performance",
    "test:dashboard": "node test-runner.js",
    "test:ci": "node test-runner.js ci"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest",
    "puppeteer-real-browser": "latest"
  },
  "devDependencies": {
    "typescript": "^5.5.3",
    "@types/node": "latest",
    "tsx": "latest",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.5",
    "@jest/globals": "^29.7.0"
  },
  "keywords": [
    "mcp",
    "puppeteer",
    "browser-automation"
  ],
  "author": "withlinda13",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withLinda/puppeteer-real-browser-mcp-server.git"
  },
  "homepage": "https://github.com/withLinda/puppeteer-real-browser-mcp-server",
  "bugs": {
    "url": "https://github.com/withLinda/puppeteer-real-browser-mcp-server/issues"
  },
  "bin": {
    "puppeteer-real-browser-mcp-server": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
